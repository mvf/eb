1 VERSIONINFO
	FILEVERSION @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,0
	PRODUCTVERSION @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,0
	FILEFLAGSMASK	0x3fL
#ifdef _DEBUG
	FILEFLAGS		0x1L
#else
	FILEFLAGS		0x0L
#endif	/* _DEBUG */
	FILEOS			0x4L
	FILETYPE		0x2L
	FILESUBTYPE		0x0L
BEGIN
	BLOCK "StringFileInfo"
	BEGIN
		BLOCK "041104b0"
		BEGIN
			VALUE "Comments", ""
			VALUE "CompanyName", ""
			VALUE "FileDescription", "EB Library"
			VALUE "FileVersion", "@FILEVERSION@"
			VALUE "InternalName", "@PROJECT_NAME@"
			VALUE "LegalCopyright", "Copyright (c) 1997-2010  Motoyuki Kasahara."
			VALUE "LegalTrademarks", ""
			VALUE "OriginalFilename", "@PROJECT_NAME@@CMAKE_SHARED_LIBRARY_SUFFIX@"
			VALUE "ProductName", "EB Library"
			VALUE "ProductVersion", "@PROJECT_VERSION@"
		END
	END
	BLOCK "VarFileInfo"
	BEGIN
		VALUE "Translation", 0x411, 1200
	END
END
